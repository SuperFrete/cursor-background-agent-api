{
  "name": "@superfrete/cursor-api-client",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./api-client": {
      "types": "./dist/api-client.d.ts",
      "import": "./dist/api-client.js",
      "require": "./dist/cjs/api-client.js"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cjs/cli.js"
    }
  },
  "bin": {
    "cursor-api": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "postbuild": "node -e \"require('fs').mkdirSync('dist/cjs',{recursive:true});require('fs').writeFileSync('dist/cjs/package.json', JSON.stringify({type:'commonjs'}))\"",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "mcp-server": "tsx src/cli.ts mcp-server",
    "mcp-server:dev": "tsx src/cli.ts mcp-server --verbose",
    "test": "vitest",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "yargs": "^18.0.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "cursor",
    "api",
    "background-composer"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "version": "1.0.6"
}
